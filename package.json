    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "typescript": "^5.2.2",
    "validate-branch-name": "^1.3.0",
